<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Use name</th>
                <th scope="col">@</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(function(obj) { %>
                <tr>
                    <td><%= obj.name %></td>
                    <td><%= obj.userName %></td>
                    <td>
                        <button type="button" class="btn btn-primary">Render</button>
                    </td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</div>